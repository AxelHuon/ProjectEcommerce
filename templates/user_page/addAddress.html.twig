{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}

<div class="login-box">
    <div class="form-row">
        <h1 class="">Add Address </h1>
    </div>
    {{ form_start(form) }}
    <div class="form-row">
        {{ form_row(form.address_name) }}
        {{ form_row(form.address) }}
    </div>
    <div class="form-row">
        {{ form_row(form.city) }}
        {{ form_row(form.cp) }}
    </div>
    <button class="btn-custom btn-black" type="submit" >Save</button>
    {{ form_end(form) }}
    </div>
{% endblock %}
